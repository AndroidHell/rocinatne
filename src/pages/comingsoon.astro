---
import Containter from "../components/Container.astro";
import BaseLayout from "../layouts/BaseLayout.astro";
---

<BaseLayout title="Heros">
  <Containter>
    <section class="mt-8 bg-white dark:bg-gray-800">
      <div
        class="mx-auto flex max-w-screen-xl flex-col items-center px-4 py-8 sm:py-16 lg:px-6"
      >
        <div class="mb-8 max-w-screen-md lg:mb-16">
          <h1
            class="mb-4 text-4xl font-extrabold tracking-tight text-gray-900 dark:text-white"
          >
            Coming soon...
          </h1>
          <p class="text-gray-500 dark:text-gray-400 sm:text-xl"></p>
        </div>
      </div>
    </section>
  </Containter>
</BaseLayout>
