  <div class="absolute inset-0 backdrop-blur-md"></div>
  <!-- Content -->
  <div
    class="relative z-10 mx-auto max-w-screen-xl px-4 py-16 sm:py-32 lg:px-6"
  >
    <div class="mx-auto max-w-screen-sm text-center">
      <h2
        class="mb-4 text-4xl font-extrabold leading-tight tracking-tight text-gray-200 dark:text-white"
